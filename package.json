{
  "name": "@omnidotdev/template-tanstack-start",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --names \"GRAPHQL-CODEGEN,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"bun run graphql:generate:watch\" \"vite dev\"",
    "build": "vite build",
    "serve": "vite preview",
    "start": "node .output/server/index.mjs",
    "icons:generate": "bun scripts/generateIcons.ts",
    "graphql:generate": "NODE_PATH=src/ dotenv -e .env.development -- graphql-codegen --config src/lib/graphql/codegen.config.ts",
    "graphql:generate:watch": "bun graphql:generate -w",
    "format": "biome format --write",
    "lint": "biome lint",
    "check": "biome check --diagnostic-level=error src",
    "knip": "knip-bun --tags=-knipignore",
    "pwa:audit": "unlighthouse-ci --site https://localhost:3000",
    "test": "bun test src/__tests__/unit src/__tests__/components",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:e2e": "NODE_ENV=test playwright test",
    "test:e2e:ui": "NODE_ENV=test playwright test --ui",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@faker-js/faker": "^10.2.0",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/import-types-preset": "^3.0.1",
    "@graphql-codegen/plugin-helpers": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-msw": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-react-query": "^6.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@happy-dom/global-registrator": "^20.0.11",
    "@playwright/test": "^1.57.0",
    "@serwist/vite": "^9.5.0",
    "@tanstack/devtools-vite": "^0.4.0",
    "@tanstack/react-devtools": "0.9.0",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router-devtools": "^1.145.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@unlighthouse/cli": "^0.17.4",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "knip": "^5.80.0",
    "msw": "^2.12.7",
    "serwist": "^9.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-mkcert": "^1.17.9"
  },
  "dependencies": {
    "@ark-ui/react": "^5.30.0",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/nitro-v2-vite-plugin": "^1.141.0",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.145.7",
    "@tanstack/react-router-ssr-query": "^1.145.7",
    "@tanstack/react-start": "^1.145.7",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sonner": "^2.0.7",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^6.0.3",
    "zod": "^4.3.5"
  }
}
