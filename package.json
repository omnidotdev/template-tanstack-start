{
  "name": "@omnidotdev/template-tanstack-start",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --names \"GRAPHQL-CODEGEN,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"bun run graphql:generate:watch\" \"vite dev\"",
    "build": "vite build",
    "serve": "vite preview",
    "start": "node .output/server/index.mjs",
    "graphql:generate": "NODE_PATH=src/ dotenv -e .env.development -- graphql-codegen --config src/lib/graphql/codegen.config.ts",
    "graphql:generate:watch": "bun graphql:generate -w",
    "format": "biome format --write",
    "lint": "biome lint",
    "check": "biome check",
    "knip": "knip-bun",
    "prepare": "husky"
  },
  "dependencies": {
    "@ark-ui/react": "^5.29.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/nitro-v2-vite-plugin": "^1.139.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.139.0",
    "@tanstack/react-router-ssr-query": "^1.139.0",
    "@tanstack/react-start": "^1.139.0",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.2-beta.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.4",
    "jose": "^6.1.2",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/import-types-preset": "^3.0.1",
    "@graphql-codegen/plugin-helpers": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-msw": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.5",
    "@graphql-codegen/typescript-react-query": "^6.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/react-devtools": "0.8.1",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.139.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "knip": "^5.70.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-mkcert": "^1.17.9"
  }
}
